{
  "name": "designvote-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.0.2",
    "aws-sdk": "^2.840.0",
    "framer-motion": ">=3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.0",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^3.0.0",
    "react-query": "^3.6.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.2"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "husky": "^5.0.9",
    "is-ci": "^2.0.0",
    "is-ci-cli": "^2.1.2",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "msw": "^0.26.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "whatwg-fetch": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "lint-staged": "lint-staged",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "validate": "npm-run-all --parallel lint test:coverage && node ./scripts/build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "**/*.+(js|jsx|ts|tsx|md|json)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
